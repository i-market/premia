<!--HEADER-->
<header class="header">
    <div class="wrap">
        <div class="opacity_bars"></div>
        <a class="logo_mobile" href="{{ path('/') }}">
            <img src="{{ asset('images/logo.png') }}" alt="">
        </a>
        <div class="left">
            <div class="social_icons">
                {% include partial('social_icons.twig') %}
            </div>
            <a href="#five" class="scheme btn">схема проезда</a>
        </div>
        <nav class="main_menu cf">
            <ul>
                {# TODO hidden link #}
                <li><a href="#">о комплексе</a></li>
                {% set item = menu.rent %}
                <li><a href="{{ item.uri }}" class="{{ item.is_active ? 'active' }}">Арендаторам</a></li>
                <li class="logo">
                    <a href="{{ path('/') }}"></a>
                </li>
                {% set item = menu.for_customers %}
                <li><a href="{{ item.uri }}" class="{{ item.is_active ? 'active' }}">Покупателям</a></li>
                {# TODO hidden link #}
                <li><a href="#">Контакты</a></li>
            </ul>
        </nav>
        <div class="right">
            <p class="tel"><a href="tel:+7 (495) 223-61-23">+7 (495) 223-61-23</a></p>
            <div class="lang_block">
                <div class="flag"><img src="{{ asset('images/lang.png') }}" alt=""></div>
                <div class="blcok_choose_lang">
                    <span class="lang">RUS</span>
                    <div class="dd_choose_lang">
                        {# TODO international pages #}
                        <a href="#"><img src="{{ asset('images/az.png') }}" alt="">Az</a>
                        <a href="#"><img src="{{ asset('images/eng.png') }}" alt="">Eng</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</header>
